{
  "name": "@activewidgets/jquery",
  "version": "3.0.7",
  "description": "Datagrid component for jQuery",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "*/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist/* && rollup -c && node rewrite-map.js",
    "output": "npm run build && rimraf out/* && build-examples",
    "prepack": "npm run build && rimraf out/*",
    "start": "serve-examples",
    "test": "npm run output && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.0.7",
    "@activewidgets/frameworks": "3.0.7",
    "htm": "3.0.4",
    "preact": "10.4.4"
  },
  "peerDependencies": {
    "jquery": "*"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.10",
    "@activewidgets/options": "3.0.7",
    "@activewidgets/puppeteer": "0.0.5",
    "@activewidgets/testing": "0.0.4",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-node-resolve": "^11",
    "@testing-library/preact": "^2",
    "express": "^4",
    "jquery": "^3",
    "marked": "^1",
    "open": "^7",
    "parcel-bundler": "^1.12",
    "postcss-import": "^12",
    "rimraf": "^3",
    "rollup": "^2",
    "rollup-plugin-postcss": "^3",
    "rollup-plugin-sourcemaps": "^0.6",
    "rollup-plugin-terser": "^7",
    "style-inject": "^0.3"
  },
  "alias": {
    "@activewidgets/jquery": "./"
  },
  "jest": {
    "projects": [
      {
        "displayName": "Unit",
        "preset": "@activewidgets/testing",
        "moduleNameMapper": {
          "@activewidgets/components": "<rootDir>/js",
          "@activewidgets/testing": "<rootDir>/test/adapter/jquery.js"
        },
        "testMatch": [
          "<rootDir>/test/options/*/unit.js"
        ]
      },
      {
        "displayName": "Visual",
        "preset": "@activewidgets/puppeteer",
        "testMatch": [
          "<rootDir>/test/visual/**/*.js"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/jquery.git"
  },
  "keywords": [
    "activewidgets",
    "datagrid",
    "jquery"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/jquery/issues"
  },
  "homepage": "https://activewidgets.com"
}
