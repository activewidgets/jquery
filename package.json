{
  "name": "@activewidgets/jquery",
  "version": "3.3.1",
  "description": "Datagrid component for jQuery",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "*/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf --glob dist/* && rollup -c",
    "examples": "npm run build && vite build",
    "prepack": "npm run build && rimraf --glob out/*",
    "preview": "npm run examples && vite preview --open /",
    "start": "vite --open /",
    "test": "npm run examples && vitest run"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.3.1",
    "@activewidgets/frameworks": "3.3.1",
    "htm": "3.1.0",
    "preact": "10.4.4"
  },
  "peerDependencies": {
    "jquery": "*"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.16",
    "@activewidgets/options": "3.3.1",
    "@activewidgets/puppeteer": "0.1.2",
    "@rollup/plugin-node-resolve": "^15",
    "@rollup/plugin-terser": "^0.4",
    "@testing-library/dom": "^9",
    "jquery": "^3",
    "rimraf": "^4",
    "rollup": "^3",
    "rollup-plugin-md": "^1",
    "rollup-plugin-postcss": "^4",
    "style-inject": "^0.3",
    "vite": "^4"
  },
  "overrides": {
    "jest": "npm:style-inject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/activewidgets/jquery.git"
  },
  "keywords": [
    "activewidgets",
    "datagrid",
    "jquery"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/jquery/issues"
  },
  "homepage": "https://activewidgets.com"
}
